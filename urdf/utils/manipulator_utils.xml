    <!--inertial_manip_support_link-->
    <xacro:macro name="inertial_manipulator_support_link" params="mass">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="${mass}" />
            <inertia ixx="${(1/12) * mass * ((0.12  * 0.12)  + (0.05 * 0.05))}" ixy="0.0" ixz="0.0"
                  iyy="${(1/12) * mass * ((0.125 * 0.125) + (0.05 * 0.05))}" iyz="0.0"
                  izz="${(1/12) * mass * ((0.125 * 0.125) + (0.12  * 0.12))}"/>
        </inertial>
    </xacro:macro>

